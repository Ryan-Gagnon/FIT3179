{
 "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
 "width": 500,
 "height": 400,
 "title": "Chipotle Stores by State Population",
 "data": {"url": "state_data.csv"},
 "mark": "circle",
 "encoding": {
 "x": {
  "field": "pop",
   "type": "quantitative",
   "title": "Population"
   },
 "y": {
  "field": "stores",
   "type": "quantitative",
   "title": "Number of Stores"
   },
 "color": {
  "field": "state",
  "type": "nominal"
   },
 "size": {
  "field": "statepop",
  "type": "quantitative"
 }
 }
}
